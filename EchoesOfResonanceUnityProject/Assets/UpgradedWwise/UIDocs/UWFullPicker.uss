.AudioEnumTreeView {
    width: 18%;
    height: 100%;
    flex-grow: 0;
    flex-basis: auto;
    color: rgb(175, 175, 175);
    font-size: 13px;
    background-color: rgba(56, 56, 56, 0);
    flex-direction: row-reverse;
    align-items: flex-start;
    justify-content: center;
    align-self: flex-start;
    align-content: flex-start;
    overflow: hidden;
    flex-shrink: 1;
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Fonts/arial.ttf?fileID=12800000&guid=3b7277e94a4c541b28a7b0b9cad075d4&type=3#arial");
    letter-spacing: -1px;
    -unity-text-outline-color: rgba(255, 255, 255, 0.88);
    -unity-text-outline-width: 0.5px;
    word-spacing: -1px;
}

.Divider {
    flex-grow: 1;
    position: absolute;
    left: 0;
    height: 100%;
    width: 2px;
    background-color: rgb(120, 120, 120);
    align-self: center;
}

.Label {
    align-items: stretch;
    align-self: center;
    justify-content: center;
    bottom: auto;
    white-space: normal;
    position: absolute;
    left: 0;
    top: auto;
    -unity-text-align: middle-center;
    -unity-font-style: bold;
    width: 17.5%;
    font-size: 125%;
    text-overflow: clip;
    height: 100%;
    flex-basis: 0;
    flex-direction: row;
    overflow: hidden;
}
